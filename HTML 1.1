        update["drop_alignment"] = "";
        update["drop_hp"] = "";
        update["drop_saving_throws"] = "";
        update["drop_senses"] = "";
        update["drop_passive_perception"] = "";
        update["drop_skills"] = "";
        update["drop_token_size"] = "";
        setAttrs(update, {silent: true});
    };

  </script>
